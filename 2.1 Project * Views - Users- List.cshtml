@model List<Basic.Models.Data>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Control</th>
            <th scope="col">ID</th>
            <th scope="col">Username</th>
            <th scope="col">Email</th>
            <th scope="col">Password</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var user in Model){
            <tr>
                <td scope="col">
                <a class="none" asp-action="Design" asp-controller="Users" asp-route-id=@user.Id>Design</a> | <a class="none" asp-action="Delete" asp-controller="Users" asp-route-id=@user.Id>Delete</a>
                </td>
                <td scope="col">@user.Id</td>
                <td scope="col">@user.Username</td>
                <td scope="col">@user.Email</td>
                <td scope="col">@user.Password</td>
            </tr>
        }
    </tbody>
</table>

<style>
.none{
    text-decoration: none;
    font-weight: 60;
}
</style>
