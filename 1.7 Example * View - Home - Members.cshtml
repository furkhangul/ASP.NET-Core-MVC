@model IEnumerable<Basic.Models.Data>

<div class="display-6">Member List</div>
<hr />

@if (Model == null || !Model.Any())
{
    <br />
    <div class="alert alert-info">We donâ€™t find any user ! </div>
}
else
{
    <table class="table table-hover table-striped">
        <thead>
            <tr>
                <th>Username</th>
                <th>First name</th>
                <th>Last name</th>
                <th>Register Time</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var users in Model)
            {
                <tr>
                    <td>@users.Username</td>
                    <td>@users.FirstName</td>
                    <td>@users.LastName</td>
                    <td>@users.RegisterTime</td>
                </tr>
            }
        </tbody>
    </table>
}
