@model List<Code_First.Models.Author>



<table class="table">
    <thead>
        <tr>
            <th scope="col">Author Id</th>
            <th scope="col">Author Name</th>
            <th scope="col">Author Age</th>
            <th scope="col">Author Bio</th>
            <th scope="col">Author Books</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var author in Model)
        {
            <tr>
                <td>@author.AuthorId</td>
                <td>@author.AuthorName</td>
                <td>@author.Age</td>
                <td>@author.AuthorBio</td>
            </tr>
        }

</table>